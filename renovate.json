{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "github-actions": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "groupName": "github-actions"
    },
    {
      "matchManagers": ["gomod"],
      "groupName": "go-modules"
    },
    {
      "matchManagers": ["npm"],
      "groupName": "npm-packages"
    }
  ]
}
