# Yosai Dashboard Environment Variables

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=1
SECRET_KEY=
# Provide a strong 32+ character value for production

# Yosai Configuration
YOSAI_ENV=development
YOSAI_CONFIG_FILE=config/config.yaml

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=yosai_db
DB_USER=yosai_user
DB_PASSWORD=change-me

# Auth0 Configuration (Development)
AUTH0_CLIENT_ID=dev-client-id
AUTH0_CLIENT_SECRET=dev-client-secret
AUTH0_DOMAIN=dev-domain.auth0.com
AUTH0_AUDIENCE=dev-audience
# Identifier for the Auth0 API

# Security
WTF_CSRF_ENABLED=True

# Additional
DEBUG=1

# Frontend Configuration
REACT_APP_API_BASE=/api/v1
# Base path for frontend API requests
