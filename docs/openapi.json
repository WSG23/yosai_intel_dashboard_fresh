    "schemas": {
      "AccessEvent": {
        "properties": {
          "access_result": {
            "type": "string"
          },
          "door_id": {
            "type": "string"
          },
          "event_id": {
            "type": "string"
          },
          "person_id": {
            "type": "string"
          },
          "timestamp": {
            "format": "date-time",
            "type": "string"
          }
