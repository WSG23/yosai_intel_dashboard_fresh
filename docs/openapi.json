          "event_id",
          "timestamp",
          "person_id",
          "door_id",
          "access_result"
        ],
        "type": "object"
      },
      "ErrorDetails": {
        "type": "string"
      },
      "ErrorResponse": {
        "properties": {
          "code": {
            "enum": [
              [
                "bad_request",
                "invalid_payload",
                "invalid_plugin",
                "invalid_type",
                "missing_file_id",
                "no_data",
                "no_file",
                "no_file_id",
                "no_file_selected",
                "no_files",
                "not_found",
                "read_error",
                "server_error",
                "unsupported_type"
              ]
            ],
            "type": "string"
          },
          "details": {
            "$ref": "#/components/schemas/ErrorDetails"
          },
          "message": {
            "type": "string"
          }
        },
        "required": [
          "code",
          "message"
        ],
        "type": "object"
      }
    }
  },
